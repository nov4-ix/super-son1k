{
  "version": 2,
  "name": "son1kvers3-enhanced",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/classic",
      "dest": "/index.html"
    },
    {
      "src": "/nexus",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "GENERATE_SOURCEMAP": "false",
      "CI": "false"
    }
  }
}
