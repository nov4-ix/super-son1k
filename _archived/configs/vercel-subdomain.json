{
  "version": 2,
  "name": "son1kvers3-enhanced",
  "builds": [
    {
      "src": "frontend/build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/build/$1"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://backend-api.son1kvers3.com",
    "REACT_APP_VERSION": "2.0",
    "REACT_APP_ENVIRONMENT": "production"
  },
  "alias": ["enhanced.son1kvers3.com", "v2.son1kvers3.com"]
}
